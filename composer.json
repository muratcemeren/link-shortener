{
    "name": "muratcemeren/url-shortener",
    "description": "Self-hosted URL shortening service with analytics dashboard",
    "type": "project",
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "scripts": {
        "format": "php-cs-fixer fix",
        "format-check": "php-cs-fixer fix --dry-run --diff"
    }
}